<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true">
	<class name="Libragri.AuthenticationDomain.Model.UserActivationRequest,Libragri.AuthenticationDomain.Model" table="UserActivationRequest" >
		<id name="Id" column="Id" type="Guid" unsaved-value="00000000-0000-0000-0000-000000000000">
			<generator class="guid.comb" />
		</id>
		<property name="Key" type="string" column="KEY"/>
		<property name="FromDate" type="date" column="FROMDATE"/>
		<!-- one to one -->
		<many-to-one name="user" cascade="all-delete-orphan" column="user_Id" class="Libragri.AuthenticationDomain.Model.User,Libragri.AuthenticationDomain.Model"/>
	</class>
</hibernate-mapping>
