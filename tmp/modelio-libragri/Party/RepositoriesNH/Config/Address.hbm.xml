<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true">
	<class name="Smag.PartyDomain.Model.Address,Smag.PartyDomain.Model" table="Address" >
		<id name="Id" column="Id" type="Guid" unsaved-value="00000000-0000-0000-0000-000000000000">
			<generator class="guid.comb" />
		</id>
		<property name="Ligne1" type="string" column="LIGNE1"/>
		<many-to-one name="country" cascade="none" column="country_Id" class="Smag.PartyDomain.Model.Country,Smag.PartyDomain.Model"/>
		<!-- many to many -->
		<set name="purposes" lazy="true">
			<key column="purposes_Address_Id" />
			<many-to-many class="Smag.PartyDomain.Model.Purpose,Smag.PartyDomain.Model" />
		</set>
		<set name="adressPurpose" cascade="all-delete-orphan" lazy="true">
			<key column="Address_adressPurpose_Id" />
			<one-to-many class="Smag.PartyDomain.Model.AdressPurpose,Smag.PartyDomain.Model" />
		</set>
	</class>
</hibernate-mapping>
