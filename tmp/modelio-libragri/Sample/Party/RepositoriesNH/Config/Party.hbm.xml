<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true">
	<class name="PartyDomain.Model.Party,PartyDomain.Model" table="Party" >
		<id name="Id" type="guid" column="Id" generator="guid.comb" />
		<property name="Name" type="string" column="NAME"/>
		<set name="Addresses" cascade="all-delete-orphan" lazy="true">
			<key column="Party_Addresses_Id" />
			<one-to-many class="PartyDomain.Model.Address,PartyDomain.Model" />
		</set>
	</class>
</hibernate-mapping>
